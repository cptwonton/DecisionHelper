<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cs465.decisionhelper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DecisionHelperActivity"
            android:label="@string/decision_helper" />
        <activity
            android:name=".pq_start"
            android:label="@string/personality_quiz" />
        <activity
            android:name=".SettingsActivity"
            android:label="@string/settings" />
        <activity
            android:name=".HelpActivity"
            android:label="@string/help" />
        <activity
            android:name=".SpinnerActivity"
            android:exported="true"
            android:label="@string/dh_new_choice"
            android:parentActivityName=".MainActivity">

            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.cs465.decisionhelper.MainActivity" />
        </activity>
        <activity android:name=".pq_questions" />
        <activity android:name=".pq_results"></activity>
    </application>

</manifest>