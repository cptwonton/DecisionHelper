<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/dh_homepage_content_frame"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

<Button
    android:id="@+id/dh_homepage_btn_newdecision"
    android:text="@string/dh_homepage_btn_newdecisiontext"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:textSize="20sp"
    android:layout_marginTop="30dp"/>

<TextView
    android:id="@+id/dh_homepage_tv_mydecisions"
    android:text="@string/dh_homepage_tv_mydecisionstext"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="30dp"
    android:textSize="20sp" />



    <Button
        android:text="Decide which job to take"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:onClick="dh_homepage_btn_jobdecisionOnClick"/>




    <TextView
        android:id="@+id/dh_homepage_tv_sharedwithme"
        android:text="@string/dh_homepage_tv_sharedwithme"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:textSize="20sp" />


</LinearLayout>


