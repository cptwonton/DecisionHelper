<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="1"
    android:orientation="vertical">

    <TextView
        android:text="@string/dh_X"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView4"
        android:textSize="18sp"
        android:layout_weight="0.15" />

    <Button
        android:text="@string/dh_add_edit_attributes"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button9"
        android:layout_weight="0.10" />

    <Button
        android:text="@string/dh_rank"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button10"
        android:layout_weight="0.09" />

    <Button
        android:text="@string/dh_new_choice"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button11"
        android:layout_weight="0.09" />

    <Button
        android:text="@string/dh_view_results"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button12"
        android:layout_weight="0.09" />

    <Button
        android:text="@string/dh_share"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button13"
        android:layout_weight="0.09" />

    <Button
        android:text="@string/dh_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button8" />

</LinearLayout>