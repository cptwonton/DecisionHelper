<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/rank_factor"
    android:weightSum="1">


    <LinearLayout
        android:id="@+id/dh_factors_list"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center">
        <Button
            android:id="@+id/dh_rank_factors_btn_OK"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/btn_OK"
            android:layout_gravity="center_horizontal"
            android:onClick="dh_rank_factors_btn_OKOnClick"/>

        <Button
            android:id="@+id/dh_rank_factors_btn_cancel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/btn_cancel"
            android:layout_gravity="center_horizontal"
            android:onClick="dh_rank_factors_btn_cancelOnClick"/>
    </LinearLayout>


</LinearLayout>