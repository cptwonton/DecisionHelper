<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="1">


    <android.support.v7.widget.Toolbar
        android:id="@+id/dh_weigh_possibility_ab"
        android:background="?attr/colorPrimary"
        android:layout_height="wrap_content"
        android:layout_width="match_parent">

        <TextView
            android:id="@+id/dh_rank_factors_tv_titlebar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="@string/dh_rank_factors_rank"
            android:textColor="@color/colorWhite"
            android:textSize = "26sp"
            android:gravity = "center" />

    </android.support.v7.widget.Toolbar>

    <TextView
        android:id="@+id/textView10"
        android:text="Q1. How much do you care about money"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="?attr/colorPrimary"
        android:layout_marginStart="1dp"
        android:layout_marginTop="20dp"
        android:textSize="20sp" />

    <SeekBar
        android:id="@+id/dh_rank_factors_sb_1"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="30dp" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" >
        <TextView
            android:text="@string/dh_weigh_possibility_range_0"
            android:layout_gravity="left"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginStart="23dp"
            android:id="@+id/textView15" />

        <TextView
            android:text="@string/dh_weigh_possibility_range_100"
            android:layout_gravity="right"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="30dp"
            android:id="@+id/textView14" />
    </LinearLayout>

    <TextView
        android:text="Q2. How much do you care about location"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="?attr/colorPrimary"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="50dp"
        android:textSize="20sp"

        android:id="@+id/textView18" />

    <SeekBar
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="30dp"

        android:id="@+id/seekBar1" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" >
        <TextView
            android:text="0"
            android:layout_gravity="left"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginLeft="23dp"
            android:id="@+id/textView16" />

        <TextView
            android:text="100"
            android:layout_gravity="right"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="30dp"
            android:id="@+id/textView17" />
    </LinearLayout>

    <TextView
        android:text="Q3.How much do you care about wheather"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="?attr/colorPrimary"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="50dp"
        android:textSize="20dp"

        android:id="@+id/textView19" />

    <SeekBar
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="30dp"

        android:id="@+id/seekBar2" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" >
        <TextView
            android:text="0"
            android:layout_gravity="left"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginLeft="23dp"
            android:id="@+id/textView20" />

        <TextView
            android:text="100"
            android:layout_gravity="right"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="30dp"
            android:id="@+id/textView22" />
    </LinearLayout>

    <TextView
        android:text="Q4.How much do you care about security"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="?attr/colorPrimary"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="50dp"
        android:textSize="20dp"

        android:id="@+id/textView24" />

    <SeekBar
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="30dp"

        android:id="@+id/seekBar3" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" >
        <TextView
            android:text="0"
            android:layout_gravity="left"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginLeft="23dp"
            android:id="@+id/textView25" />

        <TextView
            android:text="100"
            android:layout_gravity="right"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="30dp"
            android:id="@+id/textView26" />
    </LinearLayout>




</LinearLayout>